<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         version="2.5" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>HomePageServelet</servlet-name>
  	<servlet-class>cream.servlets.CreamHomePageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>HomePageServelet</servlet-name>
  	<url-pattern>/homePageServelet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>RegisterUserServlet</servlet-name>
  	<servlet-class>cream.servlets.CreamRegisterUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RegisterUserServlet</servlet-name>
  	<url-pattern>/registerUserServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginServletName</servlet-name>
  	<servlet-class>cream.servlets.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>LoginServletName</servlet-name>
  	<url-pattern>*.login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>GetServletJSONname</servlet-name>
  	<servlet-class>cream.servlets.GetServletJSON</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GetServletJSONname</servlet-name>
  	<url-pattern>*.get</url-pattern>
  </servlet-mapping>
  
</web-app>
